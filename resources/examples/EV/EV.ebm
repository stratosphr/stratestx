<?xml version="1.0" encoding="UTF-8"?>

<model xmlns="ebm" name="ELV">

    <consts-defs>
        <const-def name="NE">
            <int value="4"/>
        </const-def>
        <const-def name="LF"> <!-- Last Floor -->
            <minus>
                <const name="NE"/>
                <int value="1"/>
            </minus>
        </const-def>
    </consts-defs>

    <sets-defs>
        <set-def name="Floors">
            <range>
                <int value="0"/>
                <const name="LF"/>
            </range>
        </set-def>
        <set-def name="DoorsState">
            <enum>
                <enum-value name="ouvertes"/>
                <enum-value name="fermees"/>
                <enum-value name="refermees"/>
            </enum>
        </set-def>
    </sets-defs>

    <funs-defs>
        <fun-def name="BC">
            <named-set name="Floors"/>
            <range>
                <int value="0"/>
                <int value="1"/>
            </range>
        </fun-def>
        <fun-def name="BM">
            <named-set name="Floors"/>
            <range>
                <int value="0"/>
                <int value="1"/>
            </range>
        </fun-def>
        <fun-def name="BD">
            <named-set name="Floors"/>
            <range>
                <int value="0"/>
                <int value="1"/>
            </range>
        </fun-def>
        <fun-def name="PE">
            <named-set name="Floors"/>
            <named-set name="DoorsState"/>
        </fun-def>
    </funs-defs>

    <vars-defs>
        <var-def name="PC">
            <named-set name="DoorsState"/>
        </var-def>
        <var-def name="Pos">
            <named-set name="Floors"/>
        </var-def>
        <var-def name="Dir">
            <set>
                <int value="-1"/>
                <int value="1"/>
            </set>
        </var-def>
    </vars-defs>

    <invariant>
        <and>
            <equals>
                <fun name="BM">
                    <const name="LF"/>
                </fun>
                <int value="0"/>
            </equals>
            <equals>
                <fun name="BD">
                    <int value="0"/>
                </fun>
                <int value="0"/>
            </equals>
            <forall>
                <vars-defs>
                    <var-def name="etage">
                        <named-set name="Floors"/>
                    </var-def>
                </vars-defs>
                <implies>
                    <and>
                        <neq>
                            <var name="etage"/>
                            <var name="Pos"/>
                        </neq>
                    </and>
                    <equals>
                        <fun name="PE">
                            <var name="etage"/>
                        </fun>
                        <enum-value name="fermees"/>
                    </equals>
                </implies>
            </forall>
            <implies>
                <equals>
                    <var name="PC"/>
                    <enum-value name="ouvertes"/>
                </equals>
                <equals>
                    <fun name="PE">
                        <var name="Pos"/>
                    </fun>
                    <enum-value name="ouvertes"/>
                </equals>
            </implies>
            <forall>
                <vars-defs>
                    <var-def name="etage">
                        <named-set name="Floors"/>
                    </var-def>
                </vars-defs>
                <neq>
                    <fun name="PE">
                        <var name="etage"/>
                    </fun>
                    <enum-value name="refermees"/>
                </neq>
            </forall>
        </and>
    </invariant>

